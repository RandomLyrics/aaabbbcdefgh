<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - ADMIN - PROMOSI</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
</head>
<body>
    <nav class="navbar navbar-default">
        <div class="container-fluid">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Promosi", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div id="navbar" class="navbar-collapse collapse">
                
            
               
                <ul class="nav navbar-nav navbar-right">
                    <li>@Html.ActionLink("Powiadomienia PUSH", "../Pushes")</li>
                    <li>@Html.ActionLink("Lista firm", "../Companies")</li>
                    <li>@Html.ActionLink("Lista branż", "../Branches")</li>
                    <li>@Html.ActionLink("Lista miast", "../Cities")</li>
                    <li class="active">@Html.ActionLink("powrót", "Index", "Admin", new { area = "" }, new { })</li>
                </ul>
            </div><!--/.nav-collapse -->
        </div><!--/.container-fluid -->
    </nav>



 

    <div class="container body-content">
        <p style="padding=10px;">
            
        </p>
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Promosi, Wszelkie prawa zastrzeżone.</p>
        </footer>
    </div>

    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</body>
</html>